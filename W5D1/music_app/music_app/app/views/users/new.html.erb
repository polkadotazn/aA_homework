<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>

Sign Up!
<br>

<% if flash[:errors] %>
  <% flash[:errors].each do |error| %>
    <%= error %>
  <% end %>
<% end %>



<form action="<%= users_url %>" method="post">
  <input
  type="hidden"
  name="authenticity_token"
  value="<%= form_authenticity_token %>">
  Email
  <input
  type="text"
  name="user[email]"
  value="">
  <br>
  Password
  <input
  type="password"
  name="user[password]"
  value="">
  <br>

  <input type="submit">

</form>
